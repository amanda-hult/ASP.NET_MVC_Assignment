@model MembersViewModel

@{
	Layout = "_PortalLayout";
	ViewData["Title"] = Model.Title;
}

@await Html.PartialAsync("Partials/Components/_AddMemberModal", Model.Member)

<section id="members">
	<header>
		<h2>Members</h2>
		<button type="button" data-modal="true" data-target="#addMemberModal" class="button btn-blue btn-add">Add Member</button>
	</header>

	<div>
		@await Html.PartialAsync("Partials/Components/_MemberCard")
	</div>

</section>